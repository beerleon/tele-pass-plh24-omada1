<?xml version="1.0" encoding="UTF-8"?>
<persistence version="1.0" xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_1_0.xsd">
  <persistence-unit name="TelePassPU" transaction-type="RESOURCE_LOCAL">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <class>model.Vehicle</class>
    <class>model.VehicleCategory</class>
    <class>model.Card</class>
    <class>model.CustPermissions</class>
    <class>model.Client</class>
    <class>model.Station</class>
    <class>model.Customer</class>
    <class>model.AppUser</class>
    <class>model.RolePermission</class>
    <class>model.UserRole</class>
    <class>model.Collection</class>
    <class>model.Module</class>
    <class>model.Payment</class>
    <class>model.ProgramRate</class>
    <class>model.Program</class>
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:derby://localhost:1527/avenue"/>
      <property name="javax.persistence.jdbc.password" value="123"/>
      <property name="javax.persistence.jdbc.driver" value="org.apache.derby.jdbc.ClientDriver"/>
      <property name="javax.persistence.jdbc.user" value="avenue"/>
    </properties>
  </persistence-unit>
</persistence>
